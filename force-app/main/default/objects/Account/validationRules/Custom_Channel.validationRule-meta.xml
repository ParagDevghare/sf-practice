<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Custom_Channel</fullName>
    <active>true</active>
    <errorConditionFormula>AND((ISPICKVAL( Type , &quot;Customer - Channel&quot;)) ,  ISCHANGED(Name))</errorConditionFormula>
    <errorMessage>Can not change Customer-Channel type account name</errorMessage>
</ValidationRule>
