<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Billing_Country</fullName>
    <active>false</active>
    <errorConditionFormula>NOT(OR(BillingCountry = &quot;US&quot;, BillingCountry=&quot;USA&quot;, ISBLANK(BillingCountry)))</errorConditionFormula>
    <errorMessage>Check billing country</errorMessage>
</ValidationRule>
